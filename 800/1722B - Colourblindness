t = int(input())
while(t>0):
    t -=1
    n = int(input())
 
    s1 = input()
    s2 = input()
    
    flag = 0
    for i in range(n):
        if (s1[i] == 'R' and s2[i] != 'R') or (s2[i] == 'R' and s1[i] != 'R'):
            flag= 1
            
    if (flag == 1):
        print("NO")
    else:
        print("YES")
